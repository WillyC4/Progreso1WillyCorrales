@model Progreso1WillyCorrales.Models.WCorrales

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>WCorrales</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
