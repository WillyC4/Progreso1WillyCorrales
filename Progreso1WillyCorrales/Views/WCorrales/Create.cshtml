                <label class="form-check-label">
                    <input class="form-check-input" asp-for="SiNo" /> @Html.DisplayNameFor(model => model.SiNo)
                </label>
            </div>
            <div class="form-group">
                <label asp-for="fecha" class="control-label"></label>
